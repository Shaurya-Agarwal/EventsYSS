<%- include("./partials/header")%>
	<h1 style="text-align:center;color:#000;margin-top:175px;">Add event </h1>
		<div style="width:50%;margin:0 auto;">
			<form  action="/" method="POST" enctype="multipart/form-data">
				<div class="form-group">
					<input class="form-control" type="text" name="event" placeholder="event name">
				</div>
				<div class="form-group">
					<input class="form-control" type="file" name="image">
				</div>
				<div class="form-group">
					<input class="form-control" type="text" name="desc" placeholder="description">
				</div>
				<button class="btn btn-large btn-primary btn-block">Submit</button>
			</form>
		</div>	
<%- include("./partials/footer")%>